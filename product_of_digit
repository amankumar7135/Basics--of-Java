import java.util.Scanner;
public class Product_of_digit {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        System.out.println("enter the number you want to konw the product: ");
        int num=sc.nextInt();
        int pro=1;

        while(num>0){
        pro=pro*num%10;
        num=num/10;
        }
        System.out.println("Product of a digit: "+pro);
    }
}
